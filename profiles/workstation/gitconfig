[core]
	excludesfile = ~/.gitignore
	fileMode = true
	filemode = false
	filemode = true
[diff]
	tool = meld
[merge]
	tool = mymeld
[mergetool "mymeld"]
	cmd = meld $LOCAL $MERGED $REMOTE
[credential]
	helper = cache --timeout=36000
[difftool]
	prompt = false
[alias]
   a = add
   d = difftool
	nfm = "!f(){ git -c core.fileMode=false $@; };f"
	s = status
	m = merge
	b = branch
	c = commit
	p = push
	f = pull
	co = checkout
   mt = mergetool	
   A = "add -A"	
   cm = "commit -m"
